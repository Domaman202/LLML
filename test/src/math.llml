// Functions "abs"

abs(v): i32 = {
    @if((>= v 0) a b)
    @label(a) = {[v]}
    @label(b) = {[- 0 v]}
}

labs(v): i64 = {
    @if((>= v 0) a b)
    @label(a) = {[v]}
    @label(b) = {[- 0 v]}
}

fabs(v): f32 = {
    @if((>= v 0.) a b)
    @label(a) = {[v]}
    @label(b) = {[- 0. v]}
}

fabsl(v): f64 = {
    @if((>= v 0.) a b)
    @label(a) = {[v]}
    @label(b) = {[- 0. v]}
}

// Functions "acos"

acosf(v: f32): f32 = {
    [+ (* (- (* (* -0.69813170079 v) v) 0.87266462599) v) 1.5707963267]
}

acos(v: f64): f64 = {
    [+ (* (- (* (* -0.69813170079 v) v) 0.87266462599) v) 1.5707963267]
}